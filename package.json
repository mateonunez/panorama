{
  "name": "panorama",
  "version": "0.0.1",
  "description": "HTTP Routes and Orama",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/mateonunez/panorama#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mateonunez/panorama.git"
  },
  "bugs": {
    "url": "https://github.com/mateonunez/panorama/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "biome check --apply ./src",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "test": "npm run check && c8 node --loader ts-node/esm --test ./src/tests/*.test.ts",
    "build": "rimraf ./dist && tsc --project tsconfig.json",
    "prepare": "husky install"
  },
  "keywords": [
    "orama",
    "plugin",
    "caching"
  ],
  "author": {
    "email": "mateonunez95@gmail.com",
    "name": "Mateo Nunez",
    "url": "https://github.com/mateonunez"
  },
  "license": "MIT",
  "dependencies": {
    "@orama/orama": "2.0.0-beta.9",
    "orama-cache": "^0.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@matteo.collina/tspl": "^0.1.0",
    "@types/node": "^20.9.0",
    "c8": "^8.0.1",
    "husky": "^8.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
